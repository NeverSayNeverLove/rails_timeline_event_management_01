<div id="wrapper">
  <div class="container pt-70 pb-70">
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header user"><%= t ".search" %></h1>
      </div>
    </div>
    <%= search_form_for @q, :url => url_for(:controller => "events_managements", :action => "search_events"), remote: true do |f| %>
      <div class="form-group modalbox search_form">
        <div class="form-group search-box">
          <%= f.label :description_cont, class: "search-label" %>
          <%= f.search_field :description_cont, class: "form-control" %>
        </div>
        <div class="form-group search-box">
          <%= f.label :place_cont, class: "search-label" %>
          <%= f.search_field :place_cont, class: "form-control" %></div>
        <%= f.submit t(".search"), class: "btn btn-color btn-md btn-signup" %>
      </div>
    <% end %>
    </br>
    <div id="result">
    </div>
  </div>
</div>

